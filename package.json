{
  "name": "slack-scrumbot",
  "version": "0.1.5",
  "description": "A Slack bot that performs a pre-standup checkin.",
  "config": {
    "waitMinutes": 10
  },
  "main": "bot.js",
  "scripts": {
    "start": "forever start bot.js",
    "stop": "forever stop bot.js",
    "watch": "nodemon bot.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smhg/slack-scrumbot.git"
  },
  "keywords": [
    "slack",
    "bot",
    "scrum",
    "standup"
  ],
  "author": "Sam Hauglustaine",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.3.15",
    "babel-preset-es2015": "6.3.13",
    "debug": "2.2.0",
    "forever": "0.15.1",
    "moment": "2.11.2",
    "slack-client": "1.5.0"
  },
  "devDependencies": {
    "nodemon": "1.8.1",
    "babel-eslint": "4.1.6",
    "eslint": "1.10.3"
  }
}
