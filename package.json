  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "debug": "^2.2.0",
    "forever": "^0.15.3",
    "moment": "^2.22.2",
    "slack-client": "^1.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "eslint": "^1.10.3",
    "nodemon": "^1.17.5"
  }
